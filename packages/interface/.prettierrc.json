{
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
    "arrowParens": "always",
    "endOfLine": "lf",
    "semi": true,
    "singleQuote": true,
    "alignObjectProperties": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "useTabs": true,
    "jsxSingleQuote": true,
    "tabWidth": 4
}
